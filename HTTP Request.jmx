def namePrefix =&quot;user_&quot;
def jobPrefix = &quot;role_&quot;
//
def newName =&quot;${namePrefix}${timestamp}_${randomeNum}&quot;
def newjob = &quot;${jobPrefix}${timestamp}_${randomeNum}&quot;
//
vars.put (&quot;name&quot;, newName)
vars.put (&quot;job&quot;,newjob)
//
Log.info (&quot;Generated Name: &quot; + newName)
log.info (&quot;Genereted Job:&quot; + newjob)</stringProp>
